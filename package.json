{
	"name": "blog-api",
	"version": "1.0.0",
	"description": "Blog created with React, Node.js, Express, MongoDB, and TypeScript",
	"main": "index.js",
	"engines": {
		"node": ">=18.16.0"
	},
	"scripts": {
		"build:assets": "ts-node src/tools/copyAssets",
		"build:sass": "node-sass src/public/styles -o dist/public/styles",
		"build": "rimraf dist && tsc -p src & npm run build:assets",
		"start": "node dist/index.js",
		"dev": "nodemon src/index.ts",
		"serverstart": "DEBUG=express-members-only:* & npm run dev",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"fix": "npm run format & npm run lint:fix",
		"prepare": "husky install",
		"populate": "ts-node src/tools/populateDatabase"
	},
	"lint-staged": {
		"*.ts": "npm run fix"
	},
	"keywords": [],
	"author": "Jack Watters",
	"license": "MIT",
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/compression": "^1.7.2",
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/express-session": "^1.17.7",
		"@types/http-errors": "^2.0.1",
		"@types/jest": "^29.5.1",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/morgan": "^1.9.4",
		"@types/node": "^18.16.3",
		"@types/node-sass": "^4.11.3",
		"@types/passport": "^1.0.12",
		"@types/passport-jwt": "^3.0.8",
		"@types/passport-local": "^1.0.35",
		"@types/passport-local-mongoose": "^6.1.1",
		"@types/shelljs": "^0.8.12",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"bcryptjs": "^2.4.3",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"express-session": "^1.17.3",
		"husky": "^8.0.0",
		"jest": "^29.5.0",
		"jsonwebtoken": "^9.0.0",
		"lint-staged": "^13.2.2",
		"node-sass": "^8.0.0",
		"nodemon": "^2.0.22",
		"passport": "^0.6.0",
		"passport-local": "^1.0.0",
		"prettier": "^2.8.8",
		"rimraf": "^5.0.0",
		"sass": "^1.62.1",
		"shelljs": "^0.8.5",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"@types/multer": "^1.4.7",
		"compression": "^1.7.4",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-async-handler": "^1.2.0",
		"express-rate-limit": "^6.7.0",
		"express-validator": "^7.0.1",
		"helmet": "^6.1.5",
		"http-errors": "~1.6.3",
		"luxon": "^3.3.0",
		"mongoose": "^7.1.0",
		"morgan": "~1.9.1",
		"multer": "^1.4.5-lts.1",
		"passport-jwt": "^4.0.1"
	}
}
